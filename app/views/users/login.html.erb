<div class="col-xs-4">
  <p></p>
</div>
<div class="col-xs-4">
  <h3>Login</h3>
  <%= simple_form_for(:user, url: signin_path ) do |f| %>
    <div class="form-inputs">
      <%= f.input :email, required: true, autofocus: true %>
      <%= f.input :password, required: true %>
    </div>
    <div class="form-actions">
      <%= f.button :submit, "Enviar" %>
    </div>
  <% end %>
</div>
